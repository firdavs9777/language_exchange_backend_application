    {
      "name": "4. AI-Generated Quizzes",
      "description": "Get custom quizzes targeting your weak areas.",
      "item": [
        {
          "name": "Generate Quiz",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200 || pm.response.code === 201) {",
                  "    var jsonData = pm.response.json();",
                  "    if (jsonData.data && jsonData.data.quiz && jsonData.data.quiz._id) {",
                  "        pm.collectionVariables.set('quizId', jsonData.data.quiz._id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"type\": \"weak_areas\",\n  \"questionCount\": 10,\n  \"difficulty\": \"adaptive\",\n  \"focusAreas\": [\"past_tense\", \"vocabulary\"]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/generate",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "generate"]
            },
            "description": "Generate a custom quiz based on weak areas.\n\nTypes: weak_areas, vocabulary, recent_content, mixed"
          },
          "response": []
        },
        {
          "name": "Generate Vocabulary Quiz",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"type\": \"vocabulary\",\n  \"questionCount\": 10,\n  \"difficulty\": \"medium\",\n  \"vocabularyIds\": [\"vocab_id_1\", \"vocab_id_2\"]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/generate",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "generate"]
            },
            "description": "Generate quiz from specific vocabulary items."
          },
          "response": []
        },
        {
          "name": "Get AI Quizzes",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/ai?limit=10",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "ai"],
              "query": [
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get list of pending AI-generated quizzes."
          },
          "response": []
        },
        {
          "name": "Start Quiz",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/ai/{{quizId}}/start",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "ai", "{{quizId}}", "start"]
            },
            "description": "Start a quiz attempt and get questions (without answers)."
          },
          "response": []
        },
        {
          "name": "Submit Answer",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"questionIndex\": 0,\n  \"answer\": \"fui\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/ai/{{quizId}}/answer",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "ai", "{{quizId}}", "answer"]
            },
            "description": "Submit answer for a single question and get immediate feedback."
          },
          "response": []
        },
        {
          "name": "Complete Quiz",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"answers\": [\"fui\", \"comió\", \"hablaron\", \"escribí\", \"leímos\"],\n  \"timeSpent\": 245\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/ai/{{quizId}}/complete",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "ai", "{{quizId}}", "complete"]
            },
            "description": "Complete quiz with all answers and get final results."
          },
          "response": []
        },
        {
          "name": "Get Quiz Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/learning/quizzes/ai/stats",
              "host": ["{{baseUrl}}"],
              "path": ["learning", "quizzes", "ai", "stats"]
            },
            "description": "Get user's AI quiz statistics."
          },
          "response": []
        }
      ]
