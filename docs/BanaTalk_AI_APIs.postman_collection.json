    {
      "name": "6. Enhanced Translation",
      "description": "Context-aware translations with educational explanations.",
      "item": [
        {
          "name": "Enhanced Translation",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"text\": \"It's raining cats and dogs\",\n  \"sourceLanguage\": \"en\",\n  \"targetLanguage\": \"es\",\n  \"nativeLanguage\": \"en\",\n  \"includeBreakdown\": true,\n  \"includeGrammar\": true,\n  \"includeIdioms\": true\n}"
