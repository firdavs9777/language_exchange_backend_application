    {
      "name": "5. Speech Features",
      "description": "Text-to-speech, speech-to-text, and pronunciation evaluation.",
      "item": [
        {
          "name": "Generate TTS Audio",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"text\": \"Hola, ¿cómo estás?\",\n  \"language\": \"es\",\n  \"voice\": \"nova\",\n  \"speed\": 1.0,\n  \"format\": \"mp3\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/speech/tts",
              "host": ["{{baseUrl}}"],
              "path": ["speech", "tts"]
            },
            "description": "Generate text-to-speech audio.\n\nVoices: alloy, echo, fable, onyx, nova, shimmer\nSpeed: 0.25 to 4.0\nFormat: mp3, opus, aac, flac"
          },
          "response": []
        },
        {
          "name": "Transcribe Audio (STT)",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "audio",
                  "type": "file",
                  "src": "/path/to/your/audio.mp3",
                  "description": "Audio file (mp3, wav, webm, m4a, ogg, flac)"
                },
                {
                  "key": "language",
                  "value": "es",
                  "type": "text",
                  "description": "Language code (optional, auto-detected)"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/speech/stt",
              "host": ["{{baseUrl}}"],
              "path": ["speech", "stt"]
            },
            "description": "Transcribe speech to text using Whisper."
          },
          "response": []
        },
        {
          "name": "Evaluate Pronunciation",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "audio",
                  "type": "file",
                  "src": "/path/to/pronunciation.mp3",
                  "description": "User's pronunciation recording"
                },
                {
                  "key": "targetText",
                  "value": "Hola, ¿cómo estás?",
                  "type": "text",
                  "description": "Text that should be pronounced"
                },
                {
                  "key": "language",
                  "value": "es",
                  "type": "text"
                },
                {
                  "key": "source",
                  "value": "practice",
                  "type": "text",
                  "description": "vocabulary, lesson, practice, challenge"
                },
                {
                  "key": "vocabularyId",
                  "value": "",
                  "type": "text",
                  "description": "Optional vocabulary ID for tracking"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/speech/pronunciation/evaluate",
              "host": ["{{baseUrl}}"],
              "path": ["speech", "pronunciation", "evaluate"]
            },
            "description": "Evaluate pronunciation and get detailed scoring with AI feedback."
          },
          "response": []
        },
        {
          "name": "Get Pronunciation History",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/speech/pronunciation/history?language=es&limit=20&offset=0",
              "host": ["{{baseUrl}}"],
              "path": ["speech", "pronunciation", "history"],
              "query": [
                {
                  "key": "language",
                  "value": "es"
                },
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "offset",
                  "value": "0"
                }
              ]
            },
            "description": "Get user's pronunciation practice history."
          },
          "response": []
        },
        {
          "name": "Get Pronunciation Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/speech/pronunciation/stats?language=es",
              "host": ["{{baseUrl}}"],
              "path": ["speech", "pronunciation", "stats"],
              "query": [
                {
                  "key": "language",
                  "value": "es"
                }
              ]
            },
            "description": "Get user's pronunciation statistics."
          },
          "response": []
        },
        {
          "name": "Get Best Attempt",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/speech/pronunciation/best?targetText=Hola",
